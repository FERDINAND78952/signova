# Django settings
DEBUG=False
SECRET_KEY=your-secret-key-here

# Database settings
# DATABASE_URL will be set by Render in production

# Set to True when running on Render
RENDER=True

# Social Authentication Keys
# Google OAuth2
SOCIAL_AUTH_GOOGLE_OAUTH2_KEY=your-google-oauth2-key
SOCIAL_AUTH_GOOGLE_OAUTH2_SECRET=your-google-oauth2-secret

# Facebook OAuth2
SOCIAL_AUTH_FACEBOOK_KEY=your-facebook-app-id
SOCIAL_AUTH_FACEBOOK_SECRET=your-facebook-app-secret

# Twitter OAuth
SOCIAL_AUTH_TWITTER_KEY=your-twitter-api-key
SOCIAL_AUTH_TWITTER_SECRET=your-twitter-api-secret

# Flutterwave Payment Integration
FLW_PUBLIC_KEY=FLWPUBK_TEST-your-public-key-here
FLW_SECRET_KEY=FLWSECK_TEST-your-secret-key-here
FLW_ENCRYPTION_KEY=your-encryption-key-here
FLW_WEBHOOK_HASH=your-webhook-hash-here